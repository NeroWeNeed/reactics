#search-field-results {
    position: absolute;
    -unity-font: url("/Assets/ResourceData/Fonts/OpenSans/OpenSans-Regular.ttf");
    font-size: 12px;
    color: rgba(0.35,0.35,0.35,1.00);
    background-color: #FFFFFF;
    border-width: 1px;
    border-color: #8A8A8A;
}
#search-field-results .search-result {
padding: 2px 8px 2px 8px;
background-color: white;
}
#search-field-results .search-result:hover {
background-color: #3a72b0;
color: white;
}
